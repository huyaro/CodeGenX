package ${fullPackage};

#foreach($pkg in ${imports})
import ${pkg};
#end
import com.baomidou.mybatisplus.annotation.TableId;
import com.baomidou.mybatisplus.annotation.TableName;
import com.baomidou.mybatisplus.annotation.TableField;
import lombok.*;


/**
* <p>
    * #if("$!{table.getComment()}" != "") ${table.getComment()}
#else ${table.getName()}
#end
    * </p>
*
* @author ${author}
* @date ${date}
*/
@Data
@TableName("${table.getName()}")
public class ${table.getClassName()} #if("$!{superClass}" != "") extends ${superClass} #end{

## ----------  BEGIN 字段循环遍历  ----------
#foreach($field in ${table.getColumns()})
/**
* #if("$!{field.getComment()}" != "") ${field.getComment()} #else ${field.getName()}
#end
*/
    #if($table.getKeyColumns().contains($field.getName()))
    @TableId
    #elseif(${field.getAutoGenerated()})
    @TableId(type = IdType.Auto)
    #end
    #if(${field.getName()} != ${field.getPropName()})
    @TableField("${field.getName()}")
    #end
    #if(${field.getNullable()})
    @Nullable
    #end
private ${field.getJvmTypeName()} ${field.getPropName()};

#end
## ----------  END 普通字段循环遍历  ----------
}
